{
  "parser": "babel-eslint",
  "plugins": ["react", "prettier"],
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["prettier", "prettier/react"],
  "rules": {
    "strict":0,
    "prettier/prettier": "warn",
    "no-unused-vars": 0,
    "no-console": "off",
    "no-mixed-spaces-and-tabs": 0,
    "no-debugger": 0, 
    "semi": ["error", "always"],
    "allowImportExportEverywhere": false,
    "indent": ["warn", 2],
    "react/jsx-indent": "off",
    "react/jsx-indent-props": "off",
    "comma-dangle": [1, { 
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
    }],
    "react/prop-types": [2]
  }
}
